{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "wyrihaximus/s3-parallel-upload",
    "description": "Asyncronous upload to AWS S3",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "react/filesystem",
                "version": "dev",
                "source": {
                    "type": "git",
                    "url": "https://github.com/WyriHaximus/ReactFilesystem",
                    "reference": "refs/heads/master"
                }
            }
        }
    ],
    "require": {
	    "aws/aws-sdk-php": "~3.0",
        "wyrihaximus/react-guzzle-ring": "dev-master",
        "react/filesystem": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "~0.6",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\S3ParallelUpload\\": "src/"
        }
    }
}
